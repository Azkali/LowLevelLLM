{
  "capabilities ": "text",
  "clip_model_path": "/path/to/clip/model",
  "params": {
    "model": "llama-2-7b-chat",
    "tensor_split": [0.75, 0.25],
    "num_ctx": 2048,
    "num_gpu_layers": -1
  },
  "prompt": {
    "max_tokens": 1024,
    "temperature": 0.8,
    "top_p": 0.95,
    "top_k": 40,
    "repeat_penalty": 1.1,
    "frequency_penalty": 0.0,
    "presence_penalty": 0.0,
    "stop": ["Q:", "\n"],
    "echo": true
  },
  "chat_completion": [
    {"role": "system", "content": "You are an assistant who perfectly describes images."},
    {
        "role": "user",
        "content": "Describe this image in detail please."
    }
  ],
  "instruction": {
    "type": "video",
    "video": "Summarize the video"
  }
}
